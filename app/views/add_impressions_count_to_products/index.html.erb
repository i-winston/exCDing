<p id="notice"><%= notice %></p>

<h1>Add Impressions Count To Products</h1>

<table>
  <thead>
    <tr>
      <th>Impressions count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_impressions_count_to_products.each do |add_impressions_count_to_product| %>
      <tr>
        <td><%= add_impressions_count_to_product.impressions_count %></td>
        <td><%= link_to 'Show', add_impressions_count_to_product %></td>
        <td><%= link_to 'Edit', edit_add_impressions_count_to_product_path(add_impressions_count_to_product) %></td>
        <td><%= link_to 'Destroy', add_impressions_count_to_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

