<%= render  "endusers/header" %>
<body>
	<div class=container>
		<div class ="col-lg-3 col-md-3 user-info-form ">
			<div class = "row" >
				<div class="form-user style-2">
					<h2 style="text-align: center;">会員情報</h2>
					<table class="table" >
						<tbody>
							<tr>
								<th>Name</th>
								<th><%= "#{current_enduser.family_name}" + "#{current_enduser.first_name}" %></th>
							</tr>
							<tr>
								<th>住所</th>
								<th><%= "#{current_enduser.registration_address}" %></th>
							</tr>
							<tr>
								<th>郵便番号</th>
								<th><%= "#{current_enduser.registration_postal_code}" %></th>
							</tr>
							<tr>
								<th>電話番号</th>
								<th><%= "#{current_enduser.phone_number}" %></th>
							</tr>
							<tr>
								<th>メールアドレス</th>
								<th><%= "#{current_enduser.email}" %></th>
							</tr>
						</tbody>
					</table>
            		<%= link_to edit_enduser_path(current_enduser.id) , class: "col-xs-12 btn btn-warning glyphicon glyphicon-wrench edit_enduser_#{}" do %>
          			<% end %>
				</div>
			</div>
		</div>


		<div class ="col-lg-8 col-md-8 col-md-offset-1 col-lg-offset-1">

			<div class="row">
			  <ul id="tab-menu" class="clearfix" style="margin-bottom: 0;">
		        <li><a href="#tab1" class="active fa fa-history" style="font-size: 30px;"></a></li>
		        <li><a href="#tab2" class="fa fa-heart"  style="font-size: 30px;"></a></li>
		      </ul>

		      <div id="tab-contents" class="clearfix">
		        <div id="tab1" class="tab">
		          <p>TAB1のテキストが入ります。TAB1のテキストが入ります。TAB1のテキストが入ります。TAB1のテキストが入ります。</p>
		        </div>
		        <div id="tab2" class="tab">
		          <p>TAB2のテキストが入ります。TAB2のテキストが入ります。TAB2のテキストが入ります。TAB2のテキストが入ります。</p>
		        </div>
		      </div>

			</div>
		</div>
		</div>
	</div>

</body>


