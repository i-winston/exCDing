<div class="nested-fields well well-compact">
  <div class="form-group">
    <%= f.label :disk_name, "ディスク名" %>
    <%= f.text_field :disk_name %>
    <%= link_to_remove_association "ディスク削除", f %>
  </div>

  <div id="songs">
    <%= f.fields_for :songs do |s| %>
      <%= render 'song_fields', f: s %>
    <% end %>
    <div id="links">

      <%= link_to_add_association "曲を追加",f, :songs %>
    </div>
  </div>
</div>
