<%= render "endusers/admin" %>
<dib class=body>
	<div class=container>
		<div class ="col-md-6 col-md-offset-3 user-info-form ">
			<div class = "row" >
				<div class="form-user style-2">
					<h2 style="text-align: center;">会員情報</h2>
					<table class="table" >
						<tbody>
							<tr>
								<th>Name</th>
								<th><%= "#{@enduser.family_name}" + " " + "#{@enduser.first_name}" %></th>
							</tr>
							<tr>
								<th>Name(フリガナ)</th>
								<th><%= "#{@enduser.family_name_kana}" + " " + "#{@enduser.first_name_kana}" %></th>
							</tr>
							<tr>
								<th>住所</th>
								<th><%= "#{@enduser.registration_address}" %></th>
							</tr>
							<tr>
								<th>郵便番号</th>
								<th><%= "#{@enduser.registration_postal_code}" %></th>
							</tr>
							<tr>
								<th>電話番号</th>
								<th><%= "#{@enduser.phone_number}" %></th>
							</tr>
							<tr>
								<th>メールアドレス</th>
								<th><%= "#{@enduser.email}" %></th>
							</tr>
						</tbody>
					</table>
            		<%= link_to edit_admins_enduser_path , class: "col-xs-12 btn btn-warning glyphicon glyphicon-wrench edit_enduser_#{}" do %>
          			<% end %>
				</div>
			</div>
		</div>

		<div class ="col-lg-9 col-md-9">
			<h2>購入履歴</h2>
			<div class="row">
			</div>
		</div>
	</div>
</dib>



