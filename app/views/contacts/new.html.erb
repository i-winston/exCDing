
<%= render "endusers/header" %>

<%= form_for @contact, :url => contacts_path do |f| %>

<div class="page-header">
    <h1>お問い合わせ</h1>
  </div>
  <table class="table">
    <tr>
      <th>名前<span class="text-danger">（必須）</span></th>
      <td><%= f.text_field :name %></td>
    </tr>
    <tr>
      <th>お問い合わせ内容</th>
      <td><%= f.text_area :contact_message %></td>
    </tr>
  </table>
  <%= f.submit '確認', class: 'btn btn-primary' %>
<% end %>